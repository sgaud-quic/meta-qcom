[Unit]
Description=Mount persist partition at /var/lib/tee
Before=local-fs.target

[Mount]
DirectoryMode=0755
What=/dev/disk/by-partlabel/persist
Where=/var/lib/tee
Type=ext4

[Install]
WantedBy=local-fs.target
